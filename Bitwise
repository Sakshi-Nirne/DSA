class HelloWorld {
    public static void main(String[] args) {
        int p=6;
        int q=5;
        
        System.out.println(p&q);
        System.out.println(p|q);
        System.out.println(p^q);
        System.out.println(p<<q);
        System.out.println(p>>q);
    }
}
